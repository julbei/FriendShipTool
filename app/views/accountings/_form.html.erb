<%= simple_form_for(@accounting) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  	<%= f.input :name %>
    <%= f.input :group_id %>
    <%= f.input :person_id %>
    <%= f.input :amount %>
    <%= f.input :date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
